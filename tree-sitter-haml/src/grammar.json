{
  "name": "haml",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_node"
      }
    },
    "_node": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tag"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "tag": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SYMBOL",
          "name": "_tag"
        }
      }
    },
    "_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_tag_start"
        },
        {
          "type": "SYMBOL",
          "name": "_tag_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "class_name"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_tag_start": {
      "type": "PATTERN",
      "value": "[%\\.#]"
    },
    "_tag_name": {
      "type": "PATTERN",
      "value": "(\\w(-?))+"
    },
    "class_name": {
      "type": "PATTERN",
      "value": "\\.(\\w+(-?))+"
    },
    "comment": {
      "type": "PATTERN",
      "value": "\\/\\/((\\s+(\\w+))+)?"
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s+"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

